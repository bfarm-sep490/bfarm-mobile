{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "assets/*": [
        "./src/assets/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "constants/*": [
        "./src/constants/*"
      ],
      "context/*": [
        "./src/context/*"
      ],
      "helpers/*": [
        "./src/helpers/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "screens/*": [
        "./src/screens/*"
      ],
      "services/*": [
        "./src/services/*"
      ],
      "store/*": [
        "./src/store/*"
      ],
      "tailwind.config": [
        "./tailwind.config.js"
      ]
    },
    "strict": true
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "src/e2e/jest.setup.ts"
  ]
}